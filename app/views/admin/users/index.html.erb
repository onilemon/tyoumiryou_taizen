<h1>会員一覧</h1>
<% @users.each do |user| %>
<p>会員ID: <%= user.id %></p>
<p>ニックネーム: <%= link_to user.nickname, admin_user_path(user.id) %></p>
<p>メールアドレス: <%= user.email %></p>
<p>ステータス: <% if user.is_deleted? %></p>
                  <p class="text-secondary">退会</p>
               <% else %>
                  <p class="text-success">有効</p>
               <% end %>
<% end %>